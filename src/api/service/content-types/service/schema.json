{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "editable": false
    },
    "is_show_home": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "service_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service-detail.service-detail"
    },
    "price": {
      "type": "biginteger"
    },
    "icon_class": {
      "type": "string",
      "required": true
    }
  }
}
